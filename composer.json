{
    "name": "jhavenz/php-timeslot-reservations",
    "description": "timeslot reservation management using PHP",
    "keywords": [
        "jhavenz",
        "php-timeslot-reservations"
    ],
    "homepage": "https://github.com/jhavenz/php-timeslot-reservations",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Havens",
            "email": "mail@jhavens.tech",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "jhavenz/laravel-carbon-helpers": "^2.0",
        "nesbot/carbon": "^2.60"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Jhavenz\\PhpTimeslotReservations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jhavenz\\PhpTimeslotReservations\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
